{
	"name": "wikibase-vuejs-components",
	"version": "0.1.0",
	"description": "A library for vuejs components used in Wikibase projects. Inspired by OOUI",
	"repository": {
		"type": "git",
		"url": "git+https://gerrit.wikimedia.org/r/wikibase/vuejs-components"
	},
	"bugs": {
		"url": "https://github.com/wmde/wikibase-vuejs-components/issues"
	},
	"homepage": "https://github.com/wmde/wikibase-vuejs-components#readme",
	"keywords": [
		"vue",
		"vuejs",
		"OOUI",
		"Mediawiki",
		"Wikibase",
		"components"
	],
	"author": "The Wikidata team",
	"license": "GPL-2.0+",
	"scripts": {
		"build:components": "NODE_ENV=production vue-cli-service build --target lib --formats commonjs --name wikibase-vuejs-components src/index.ts",
		"build:types": "tsc -p tsconfig.json",
		"build": "npm-run-all build:*",
		"fix": "vue-cli-service lint --max-warnings 0 .",
		"test:lint": "vue-cli-service lint --max-warnings 0 --no-fix .",
		"test:unit": "vue-cli-service test:unit -c ./jest.config.js",
		"test": "npm-run-all test:*",
		"prepublishOnly": "npm run build"
	},
	"main": "./dist/wikibase-vuejs-components.common.js",
	"typings": "./dist/index.d.ts",
	"devDependencies": {
		"@types/jest": "^23.1.4",
		"@typescript-eslint/eslint-plugin": "^2.8.0",
		"@typescript-eslint/parser": "^2.8.0",
		"@vue/cli-plugin-babel": "^3.12.0",
		"@vue/cli-plugin-eslint": "^3.12.1",
		"@vue/cli-service": "^3.12.0",
		"@vue/eslint-config-typescript": "^4.0.0",
		"@vue/cli-plugin-unit-jest": "^3.12.0",
		"@vue/test-utils": "1.0.0-beta.29",
		"@wmde/eslint-config-wikimedia-typescript": "^0.1.1",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"babel-preset-vue": "^2.0.2",
		"eslint": "^5.16.0",
		"eslint-config-wikimedia": "^0.14.3",
		"eslint-plugin-filenames": "^1.3.2",
		"eslint-plugin-vue": "^5.2.3",
		"npm-run-all": "^4.1.5",
		"ts-jest": "^23.10.5",
		"typescript": "^3.5.3",
		"vue": "^2.6.10",
		"vue-class-component": "^6.0.0",
		"vue-cli-plugin-typescript": "0.0.1",
		"vue-property-decorator": "^7.0.0",
		"vue-template-compiler": "^2.6.10"
	},
	"peerDependencies": {
		"vue": "^2.6.10",
		"vue-property-decorator": "^7.0.0",
		"vue-class-component": "^6.0.0"
	}
}
